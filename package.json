{
  "name": "kingsukh-guesthouse-react",
  "version": "1.0.0",
  "description": "Kingsukh Guest House - A React-based PWA",
  "main": "index.tsx",
  "scripts": {
    "start": "npm run build && npm run serve",
    "build": "webpack --mode=production",
    "serve": "webpack-dev-server --mode=development",
    "test": "jest",
    "lint": "eslint . --fix",
    "generate": "pwa-asset-generator --publicPath / --prefix public/icons --outDir public/icons"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/kingsukh-guesthouse-react.git"
  },
  "author": "Achit Vaishnava",
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "webpack": "^5.75.0",
    "webpack-dev-server": "^4.11.1",
    "workbox-webpack-plugin": "^6.5.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.10",
    "eslint": "^8.33.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^29.5.0",
    "pwa-asset-generator": "^5.0.1"
  }
}
